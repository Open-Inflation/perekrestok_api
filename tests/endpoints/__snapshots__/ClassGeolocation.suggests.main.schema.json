{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "content": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string"
              },
              "fullAddress": {
                "type": "string"
              },
              "location": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "coordinates": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    }
                  }
                },
                "required": [
                  "coordinates",
                  "type"
                ]
              },
              "isMaxPrecision": {
                "type": "boolean"
              },
              "street": {
                "type": "null"
              },
              "house": {
                "type": "null"
              }
            },
            "required": [
              "fullAddress",
              "house",
              "isMaxPrecision",
              "location",
              "street",
              "title"
            ]
          }
        }
      },
      "required": [
        "items"
      ]
    }
  },
  "required": [
    "content"
  ]
}