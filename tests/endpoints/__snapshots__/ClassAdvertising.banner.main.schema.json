{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "content": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "slug": {
                "type": "string"
              },
              "title": {
                "type": "null"
              },
              "action": {
                "type": "string"
              },
              "color": {
                "type": "object",
                "properties": {
                  "red": {
                    "type": "integer"
                  },
                  "green": {
                    "type": "integer"
                  },
                  "blue": {
                    "type": "integer"
                  },
                  "alpha": {
                    "type": "integer"
                  }
                },
                "required": [
                  "alpha",
                  "blue",
                  "green",
                  "red"
                ]
              },
              "link": {
                "type": [
                  "null",
                  "string"
                ]
              },
              "webViewType": {
                "type": "string"
              },
              "textForImage": {
                "type": "null"
              },
              "images": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "image": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "integer"
                        },
                        "originName": {
                          "type": "string"
                        },
                        "fileType": {
                          "type": "string"
                        },
                        "fileSize": {
                          "type": "integer"
                        },
                        "width": {
                          "type": "integer"
                        },
                        "height": {
                          "type": "integer"
                        },
                        "cropUrlTemplate": {
                          "format": "uri",
                          "type": "string"
                        },
                        "publicUrl": {
                          "type": "null"
                        },
                        "uuid": {
                          "format": "uuid",
                          "type": "string"
                        }
                      },
                      "required": [
                        "cropUrlTemplate",
                        "fileSize",
                        "fileType",
                        "height",
                        "id",
                        "originName",
                        "publicUrl",
                        "uuid",
                        "width"
                      ]
                    },
                    "scales": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "scaleName": {
                            "type": "string"
                          },
                          "image": {
                            "type": "object",
                            "properties": {
                              "id": {
                                "type": "integer"
                              },
                              "originName": {
                                "type": "string"
                              },
                              "fileType": {
                                "type": "string"
                              },
                              "fileSize": {
                                "type": "integer"
                              },
                              "width": {
                                "type": "integer"
                              },
                              "height": {
                                "type": "integer"
                              },
                              "cropUrlTemplate": {
                                "format": "uri",
                                "type": "string"
                              },
                              "publicUrl": {
                                "type": "null"
                              },
                              "uuid": {
                                "format": "uuid",
                                "type": "string"
                              }
                            },
                            "required": [
                              "cropUrlTemplate",
                              "fileSize",
                              "fileType",
                              "height",
                              "id",
                              "originName",
                              "publicUrl",
                              "uuid",
                              "width"
                            ]
                          }
                        },
                        "required": [
                          "image",
                          "scaleName"
                        ]
                      }
                    },
                    "type": {
                      "type": "string"
                    },
                    "advToken": {
                      "type": "null"
                    }
                  },
                  "required": [
                    "advToken",
                    "image",
                    "scales",
                    "type"
                  ]
                }
              },
              "place": {
                "type": "string"
              },
              "leftSubtitle": {
                "type": "null"
              },
              "rightSubtitle": {
                "type": "null"
              },
              "content": {
                "type": "null"
              },
              "blockTypes": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "analyticsInfo": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "type": {
                      "type": "string"
                    },
                    "value": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "name",
                    "type",
                    "value"
                  ]
                }
              },
              "positions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "key": {
                      "type": "string"
                    },
                    "title": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "key",
                    "title"
                  ]
                }
              },
              "key": {
                "type": "null"
              },
              "deepLink": {
                "anyOf": [
                  {
                    "type": "null"
                  },
                  {
                    "type": "object",
                    "properties": {
                      "url": {
                        "format": "uri",
                        "type": "string"
                      },
                      "tsx_type": {
                        "type": "string"
                      },
                      "add_token_to_cookie": {
                        "type": "boolean"
                      },
                      "banner_id": {
                        "type": "integer"
                      },
                      "banner_slug": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "tsx_type"
                    ]
                  }
                ]
              }
            },
            "required": [
              "action",
              "analyticsInfo",
              "blockTypes",
              "color",
              "content",
              "deepLink",
              "id",
              "images",
              "key",
              "leftSubtitle",
              "link",
              "place",
              "positions",
              "rightSubtitle",
              "slug",
              "textForImage",
              "title",
              "webViewType"
            ]
          }
        },
        "tags": {
          "type": "array"
        }
      },
      "required": [
        "items",
        "tags"
      ]
    }
  },
  "required": [
    "content"
  ]
}