{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "content": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer"
              },
              "rating": {
                "type": "integer"
              },
              "comment": {
                "type": "string"
              },
              "username": {
                "type": "string"
              },
              "notes": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "key": {
                      "type": "string"
                    },
                    "value": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "key",
                    "value"
                  ]
                }
              },
              "createdAt": {
                "type": "integer"
              },
              "points": {
                "type": [
                  "integer",
                  "null"
                ]
              },
              "approvedImages": {
                "type": "array"
              },
              "reaction": {
                "type": "object",
                "properties": {
                  "likes": {
                    "type": "integer"
                  },
                  "dislikes": {
                    "type": "integer"
                  },
                  "balance": {
                    "type": "integer"
                  },
                  "vote": {
                    "type": "integer"
                  },
                  "isAvailable": {
                    "type": "boolean"
                  }
                },
                "required": [
                  "balance",
                  "dislikes",
                  "isAvailable",
                  "likes",
                  "vote"
                ]
              },
              "isContent": {
                "type": "boolean"
              }
            },
            "required": [
              "approvedImages",
              "comment",
              "createdAt",
              "id",
              "isContent",
              "notes",
              "points",
              "rating",
              "reaction",
              "username"
            ]
          }
        },
        "paginator": {
          "type": "object",
          "properties": {
            "step": {
              "type": "integer"
            },
            "current": {
              "type": "integer"
            },
            "nextPageExists": {
              "type": "boolean"
            }
          },
          "required": [
            "current",
            "nextPageExists",
            "step"
          ]
        }
      },
      "required": [
        "items",
        "paginator"
      ]
    }
  },
  "required": [
    "content"
  ]
}